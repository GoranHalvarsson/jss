{
  "name": "sitecore-jss-advancedapp-angular",
  "version": "0.0.1",
  "author": {
    "name": "Sitecore Corporation",
    "email": "jss@sitecore.net",
    "url": "https://jss.sitecore.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "homepage": "https://jss.sitecore.net",
  "license": "Apache-2.0",
  "config": {
    "appName": "JssAdvancedAppAngular",
    "sitecoreDistPath": "/dist/JssAdvancedAppAngular",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "buildArtifactsPathDev": "./dist/angular-dev",
    "buildArtifactsPathProd": "./dist/angular-prod",
    "language": "en"
  },
  "scripts": {
    "ng": "ng",
    "prebuild": "node ./internals/build/prebuild.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "ng serve --base-href=/",
    "dev": "ng serve --base-href=/",
    "setup": "scjss-setup",
    "deploy-config": "scjss-verify-setup && cross-env-shell scjss-deploy --sourcePath ./sitecore/config --destinationPath {sitecoreInstancePath}$npm_package_config_sitecoreConfigPath",
    "deploy-codefirst": "npm run build:ssr && npm run generate-manifest && npm run generate-package-from-manifest && npm run deploy:from-package",
    "deploy-codefirst:files-only": "npm run build:ssr && npm run generate-package-from-manifest:files-only && npm run deploy:from-package",
    "deploy-codefirst:items-only": "npm run generate-manifest && npm run generate-package-from-manifest:items-only && npm run deploy:from-package",
    "deploy-codefirst:no-content": "npm run generate-manifest:no-content && npm run generate-package-from-manifest:items-only && npm run deploy:from-package",
    "deploy-sitecorefirst": "scjss-verify-setup && npm run build:ssr && npm run deploy:copy-files",
    "pull-route-data": "cross-env-shell node ./internals/build/pull.js --language $npm_package_config_language --layoutServiceHost {layoutServiceHost} --apiKey {apiKey} --outputPath $npm_package_config_routeDataDir",
    "pull-all-route-data": "cross-env-shell node ./internals/build/pull.js --all --language $npm_package_config_language --layoutServiceHost {layoutServiceHost} --apiKey {apiKey} --routeDataDir $npm_package_config_routeDataDir",
    "generate-manifest": "cross-env-shell scjss-manifest generate --files ./sitecore/manifest/**/*.sitecore.ts --compilers ts-node/register --appName $npm_package_config_appName --viewEngine react --outputPath ./dist/manifest/sitecore-import.json --language $npm_package_config_language --pipelinePatchFiles ./sitecore/pipelines/**/*.patch.ts",
    "generate-manifest:no-content": "npm run generate-manifest -- --excludeItems --excludeMedia",
    "generate-package-from-manifest": "scjss-verify-setup && cross-env-shell scjss-package-generate --buildPath $npm_package_config_buildArtifactsPathProd --outputPath ./dist/update-package --sitecoreDistPath $npm_package_config_sitecoreDistPath --appName $npm_package_config_appName --manifestPath ./dist/manifest --manifestFileName sitecore-import.json",
    "generate-package-from-manifest:files-only": "scjss-verify-setup && cross-env-shell scjss-package-generate --buildPath $npm_package_config_buildArtifactsPathProd --outputPath ./dist/update-package --sitecoreDistPath $npm_package_config_sitecoreDistPath --appName $npm_package_config_appName --noManifest",
    "generate-package-from-manifest:items-only": "scjss-verify-setup && cross-env-shell scjss-package-generate --outputPath ./dist/update-package --noDist --appName $npm_package_config_appName --manifestPath ./dist/manifest --manifestFileName sitecore-import.json",
    "deploy:from-package": "cross-env-shell scjss-package-deploy --packagePath ./dist/update-package --appName $npm_package_config_appName --shipUrl {shipUrl}",
    "deploy:copy-files": "scjss-verify-setup && cross-env-shell scjss-deploy --sourcePath $npm_package_config_buildArtifactsPathProd --destinationPath {sitecoreInstancePath}$npm_package_config_sitecoreDistPath",
    "build:ssr": "npm run build:client-and-server-bundles && npm run build:server-wrapper-bundle",
    "prebuild:client-and-server-bundles": "node ./internals/build/prebuild.js prod",
    "build:client-and-server-bundles": "cross-env-shell ng build --prod --base-href / --deploy-url $npm_package_config_sitecoreDistPath/browser --output-path=$npm_package_config_buildArtifactsPathProd/browser && cross-env-shell ng build --prod --app 1 --output-path=$npm_package_config_buildArtifactsPathProd/server --output-hashing=false",
    "build:server-wrapper-bundle": "cross-env-shell webpack --config webpack.server.config.js --progress --colors --output-path $npm_package_config_buildArtifactsPathProd"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/cdk": "^5.0.3",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/material": "^5.0.3",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/platform-server": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@sitecore-jss/sitecore-jss": "^6.0.0",
    "@sitecore-jss/sitecore-jss-angular": "^2.0.0",
    "core-js": "^2.4.1",
    "ngx-swiper-wrapper": "^5.3.0",
    "rxjs": "^5.5.2",
    "ts-loader": "^3.2.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@sitecore-jss/sitecore-jss-dev-tools": "^6.0.1",
    "@sitecore-jss/sitecore-jss-manifest": "^6.0.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chalk": "^2.1.0",
    "codelyzer": "^4.0.2",
    "cross-env": "^5.0.5",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "^5.7.0",
    "typescript": "2.4.2"
  }
}
